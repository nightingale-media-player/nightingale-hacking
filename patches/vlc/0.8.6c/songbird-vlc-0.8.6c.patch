+
+{
+	int navMinorVers = g_pNavigatorFuncs->version & 0xFF;
+	NPError err;
+	if( navMinorVers >= NPVERS_HAS_NOTIFICATION ) {
+		err = g_pNavigatorFuncs->geturlnotify(instance, url, target, notifyData);
+	}
+	else {
+		err = NPERR_INCOMPATIBLE_VERSION_ERROR;
+	}
+	return err;
+}
