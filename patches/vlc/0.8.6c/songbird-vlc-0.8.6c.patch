-                             p_this->p_vlc->psz_object_name );
-
-            for( i_index = 0; p_longopts[i_index].name; i_index++ )
-                free( (char *)p_longopts[i_index].name );
-            free( p_longopts );
-            free( psz_shortopts );
-            return -1;
-        }
-    }
-
-    /* Free allocated resources */
-    for( i_index = 0; p_longopts[i_index].name; i_index++ )
-        free( (char *)p_longopts[i_index].name );
-    free( p_longopts );
-    free( psz_shortopts );
-    if( b_ignore_errors ) free( ppsz_argv );
-
-    return 0;
