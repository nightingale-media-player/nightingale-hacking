+    /* assign plugin script root class */
+    if( (NULL != version) && (!strcmp(version, "VideoLAN.VLCPlugin.2")) )
+    {
+        /* new APIs */
+        p_scriptClass = RuntimeNPClass<LibvlcRootNPObject>::getClass();
+    }
+    else
+    {
+        /* legacy APIs */
+        p_scriptClass = RuntimeNPClass<VlcNPObject>::getClass();
+    }
