        <argsstring>(in AString aKeyString, in unsigned long aStartFrom)</argsstring>
        <name>getNextRowIndexForKeyNavigation</name>
        <param>
          <type>in AString</type>
          <declname>aKeyString</declname>
        </param>
        <param>
          <type>in unsigned long</type>
          <declname>aStartFrom</declname>
