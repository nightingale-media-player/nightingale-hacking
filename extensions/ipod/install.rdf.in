#filter substitution
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <!-- Nightingale target application. -->
    <em:targetApplication>
      <Description>
        <em:id>nightingale@getnightingale.com</em:id>
        <em:minVersion>@EXTENSION_MIN_VER@</em:minVersion>
        <em:maxVersion>@EXTENSION_MAX_VER@</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:id>@EXTENSION_UUID@</em:id>
    <em:name>iPod Device Support</em:name>
    <em:iconURL>chrome://ipod/skin/em-icon.png</em:iconURL>
    <em:description>Adds support for iPod devices.</em:description>
    <em:creator>@SB_VENDOR@</em:creator>
    <em:version>@EXTENSION_VER@</em:version>

    <em:type>2</em:type>
    <em:targetPlatform>@EXTENSION_ARCH@</em:targetPlatform>

  </Description>

</RDF>
