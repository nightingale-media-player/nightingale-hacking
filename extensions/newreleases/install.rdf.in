<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <!-- Nightingale target application. -->
    <em:targetApplication>
      <Description>
        <em:id>nightingale@getnightingale.com</em:id>
        <em:minVersion>@EXTENSION_MIN_VER@</em:minVersion>
        <em:maxVersion>@EXTENSION_MAX_VER@</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:id>@EXTENSION_UUID@</em:id>
    <em:type>2</em:type>
    <em:version>@EXTENSION_VER@</em:version>
    <em:name>New Releases</em:name>
	<em:creator>PJ Ople (pjople@songbirdnest.com)</em:creator>
	<em:contributor>zk7</em:contributor>
	<em:contributor>Steve W - Coppery theme for Nightingale</em:contributor>
	<em:description>Upcoming releases by artists in your library.</em:description>
	<em:iconURL>chrome://newreleases/skin/newReleases-icon.png</em:iconURL>
	<em:homepageURL>http://addons.songbirdnest.com/addon/1563</em:homepageURL>

  </Description>
</RDF>
