foldersync.jar:
% content foldersync  %content/
% skin    foldersync  classic/1.0 %skin/
% locale  foldersync  de     %locale/de/
% locale  foldersync  en-US  %locale/en-US/
% locale  foldersync  es-AR  %locale/es-AR/
% locale  foldersync  fr     %locale/fr/
% locale  foldersync  hr-HR  %locale/hr-HR/
% locale  foldersync  zh-CN  %locale/zh-CN/
% overlay chrome://songbird/content/xul/layoutWithBrowserOverlay.xul chrome://foldersync/content/central-overlay.xul 
@include_all_to
