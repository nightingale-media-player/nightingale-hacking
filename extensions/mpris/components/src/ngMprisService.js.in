ngMprisService.prototype.setMetadata = function(track_num) {
    var key = this._dbus.getStringArg();
    var value = this._dbus.getStringArg();
    
    var track_info = this._mediacore.sequencer.view.getItemByIndex(track_num);
    
    switch (key) {
      case 'rating':
