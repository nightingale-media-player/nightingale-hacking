#filter substitution
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:nightingale="http://www.getnightingale.com/2007/addon-metadata-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <!-- Nightingale target application. -->
    <em:targetApplication>
      <Description>
        <em:id>nightingale@getnightingale.com</em:id>
        <em:minVersion>@EXTENSION_MIN_VER@</em:minVersion>
        <em:maxVersion>@EXTENSION_MAX_VER@</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:id>@EXTENSION_UUID@</em:id>
    <em:name>Playlist Folders</em:name>
    <em:iconURL>chrome://playlistfolders/skin/icon.png</em:iconURL>
    <em:description>Sort playlists in folders.</em:description>
    <em:creator>rsjtdrjgfuzkfg</em:creator>
    <em:version>@EXTENSION_VER@</em:version>
    
    <em:translator>rsjtdrjgfuzkfg (de)</em:translator>
    <em:translator>Goofy (fr)</em:translator>
    <em:translator>gogo (hr-HR)</em:translator>
    <em:translator>DakSrbija (sr)</em:translator>
    <em:translator>Lakrits (sv-SE)</em:translator>
    <em:translator>omrakin (tr)</em:translator>
    <em:translator>yfdyh000 (zh-CN)</em:translator>
    
    <em:hidden>true</em:hidden>
    <em:appManaged>true</em:appManaged>
  </Description>
</RDF>
