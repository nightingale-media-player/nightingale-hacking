<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>plucked@songbirdnest.com</em:id>
    <em:version>1.0</em:version>

    <!-- Songbird -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>0.2</em:minVersion>
        <em:maxVersion>0.3</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:name>Plucked</em:name>
    <em:description>Plain, no styles</em:description>
    <em:locked>true</em:locked>
    <em:creator>Pioneers of the Inevitable</em:creator>

    <songbird:feathers>
      <Description>
        <songbird:skin>
          <Description>
            <songbird:name>Plucked</songbird:name>
            <songbird:internalName>plucked/0.1</songbird:internalName>
            <songbird:compatibleLayout>
              <Description>
                <songbird:layoutURL>chrome://rubberducky/content/xul/mainwin.xul</songbird:layoutURL>
                <songbird:showChrome>true</songbird:showChrome>
              </Description>
              <Description>
                <songbird:layoutURL>chrome://rubberducky/content/xul/miniplayer.xul</songbird:layoutURL>
                <songbird:showChrome>true</songbird:showChrome>
              </Description>              
            </songbird:compatibleLayout>
          </Description>
        </songbird:skin>
      </Description>
    </songbird:feathers>

    <em:hidden>true</em:hidden>
    <em:appManaged>true</em:appManaged>
  </Description>

</RDF>
